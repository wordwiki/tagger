{
    "compilerOptions": {
        "target": "es2022",
        "jsx": "react",
        "jsxFactory": "jsx",
        "jsxFragmentFactory": "jsxFragment",
        "outDir": "dist", 
        "plugins": [
            {
            "name": "typescript-deno-plugin",
            "enable": true, // default is `true`
            "importmap": "import_map.json"
        }
        ]
    }
}
